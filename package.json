{
  "name": "coherence",
  "version": "1.1.0",
  "description": "Powered by Cohere",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "stellar": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "type": "module",
  "scripts": {
    "build": "bunchee --env=COHERE_API_KEY,FISH_AUDIO_API_KEY,REFERENCE_ID,CHAT_INSTRUCT",
    "start": "bun run ./dist/index.js chat",
    "dev": "bun run --watch src/index.ts chat",
    "lint:fix": "bun run lint.mjs --fix"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.0.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.27.0",
    "@inquirer/prompts": "^3.3.0",
    "@msgpack/msgpack": "^3.1.1",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "chalk": "^5.3.0",
    "cohere-ai": "^7.7.5",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "bun-types": "^1.0.25",
    "bunchee": "^4.4.8",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
